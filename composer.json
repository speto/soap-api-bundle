{
    "name" : "webit/soap-api-bundle",
    "type": "symfony-bundle",
    "description": "SOAP API library Symfony 2 integration",
    "keywords": ["web-it","soap", "api"],
    "homepage": "http://www.web-it.eu",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Bojdo",
            "email": "daniel.bojdo@web-it.eu",
            "homepage": "http://www.web-it.eu"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/symfony": "~2.3|~3.0",
        "jms/serializer-bundle": "~0.12|~1.1",
        "webit/soap-api": "^1.0.1"
    },
    "require-dev": {
        "behat/behat": "~3.0",
        "behat/symfony2-extension": "~2.0",
        "phpunit/phpunit": "~4.2"
    },
    "autoload": {
        "psr-4": {
            "Webit\\Bundle\\SoapApiBundle\\" : "src"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-1.x": "1.x-dev",
            "dev-master": "2.x-dev"
        }
    }
}
